                    Grid.Row="5"
                    Grid.Column="6"
                    Background="Transparent"/>

            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="6"
                    Grid.Column="0"
                    Background="#8065a6d1">
                <TextBlock HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Foreground="#777777"
                           TextAlignment="Center"
                           FontSize="20">
                    <Run FontWeight="Medium" Text="6 пара"/>
                    <LineBreak/>
                    <Run Text="18:10-19:40"/>
                </TextBlock>
            </Border>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="6"
                    Grid.Column="1"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="6"
                    Grid.Column="2"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
