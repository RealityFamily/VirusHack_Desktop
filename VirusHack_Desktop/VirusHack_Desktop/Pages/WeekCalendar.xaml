                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="6"
                    Grid.Column="4"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="6"
                    Grid.Column="5"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,0,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="6"
                    Grid.Column="6"
                    Background="Transparent"/>

            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="7"
                    Grid.Column="0"
                    Background="#8065a6d1">
                <TextBlock HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Foreground="#777777"
                           TextAlignment="Center"
                           FontSize="20">
                    <Run FontWeight="Medium" Text="7 пара"/>
                    <LineBreak/>
                    <Run Text="18:30-20:00"/>
                </TextBlock>
            </Border>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="7"
                    Grid.Column="1"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="7"
                    Grid.Column="2"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="7"
                    Grid.Column="3"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="7"
                    Grid.Column="4"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="7"
                    Grid.Column="5"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,0,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="7"
                    Grid.Column="6"
                    Background="Transparent"/>

            <Border BorderThickness="0,0,1,0"
                    BorderBrush="#65A6D1"
                    Grid.Row="8"
                    Grid.Column="0"
                    Background="#8065a6d1">
                <TextBlock HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Foreground="#777777"
                           TextAlignment="Center"
                           FontSize="20">
                    <Run FontWeight="Medium" Text="8 пара"/>
                    <LineBreak/>
                    <Run Text="20:10-21:40"/>
                </TextBlock>
            </Border>
            <Border BorderThickness="0,0,1,0"
                    BorderBrush="#65A6D1"
                    Grid.Row="8"
                    Grid.Column="1"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,0"
                    BorderBrush="#65A6D1"
                    Grid.Row="8"
                    Grid.Column="2"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,0"
                    BorderBrush="#65A6D1"
                    Grid.Row="8"
