                    Grid.Row="3"
                    Grid.Column="5"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,0,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="3"
                    Grid.Column="6"
                    Background="Transparent"/>

            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="4"
                    Grid.Column="0"
                    Background="#8065a6d1">
                <TextBlock HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Foreground="#777777"
                           TextAlignment="Center"
                           FontSize="20">
                    <Run FontWeight="Medium" Text="4 пара"/>
                    <LineBreak/>
                    <Run Text="14:50-16:20"/>
                </TextBlock>
            </Border>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="4"
                    Grid.Column="1"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="4"
                    Grid.Column="2"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="4"
                    Grid.Column="3"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="4"
                    Grid.Column="4"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="4"
                    Grid.Column="5"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,0,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="4"
                    Grid.Column="6"
                    Background="Transparent"/>

            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="5"
                    Grid.Column="0"
                    Background="#8065a6d1">
                <TextBlock HorizontalAlignment="Center"
                           VerticalAlignment="Center"
                           Foreground="#777777"
                           TextAlignment="Center"
                           FontSize="20">
                    <Run FontWeight="Medium" Text="5 пара"/>
                    <LineBreak/>
                    <Run Text="16:30-18:00"/>
                </TextBlock>
            </Border>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="5"
                    Grid.Column="1"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
                    BorderBrush="#65A6D1"
                    Grid.Row="5"
                    Grid.Column="2"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,1,1"
