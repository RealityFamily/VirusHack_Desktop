            <Border BorderThickness="0,0,1,0"
                    BorderBrush="#65A6D1"
                    Grid.Row="8"
                    Grid.Column="5"
                    Background="Transparent"/>
            <Border BorderThickness="0,0,0,0"
                    BorderBrush="#65A6D1"
                    Grid.Row="8"
                    Grid.Column="6"
                    Background="Transparent"/>
